{% extends 'baseF.html.twig' %}

{% block title %}Search Results{% endblock %}

{% block body %}
    <h1>Search Results</h1>

    {% if search_results is empty %}
        <p>No results found.</p>
    {% else %}
        <ul>
            {% for result in search_results %}
                <li>{{ result }}</li>
            {% endfor %}
        </ul>
    {% endif %}
{% endblock %}
